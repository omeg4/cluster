Cell[BoxData[
 RowBox[{
  RowBox[{"ndesscale", "[", 
   RowBox[{
   "nmax_", ",", "mx_", ",", "my_", ",", "pot_", ",", "kappa_", ",", "chi_", 
    ",", "d_", ",", "eps_", ",", "c_", ",", "maxi_", ",", "vn_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"tds", ",", "tdstrans", ",", 
      RowBox[{"shift", "=", "10"}], ",", 
      RowBox[{"stranseps", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Pi", "/", "2"}], ")"}], "-", "$MachineEpsilon"}], "]"}]}], 
      ",", "evs", ",", "efs"}], "}"}], ",", 
    RowBox[{
     RowBox[{"tds", "=", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{"1", "/", "2"}], ")"}]}], "*", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "/", "mx"}], ")"}], "*", 
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"f", "[", 
               RowBox[{"x", ",", "y"}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", "2"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"y", ",", "0"}], "}"}]}], "]"}]}], "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "/", "my"}], ")"}], "*", 
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"f", "[", 
               RowBox[{"x", ",", "y"}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", "0"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"y", ",", "2"}], "}"}]}], "]"}]}]}], ")"}]}], "+", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"pot", "[", "kappa", "]"}], "[", 
           RowBox[{"d", ",", "chi"}], "]"}], "[", 
          RowBox[{"x", ",", "y"}], "]"}], "*", 
         RowBox[{"f", "[", 
          RowBox[{"x", ",", "y"}], "]"}]}], "+", 
        RowBox[{"shift", "*", 
         RowBox[{"f", "[", 
          RowBox[{"x", ",", "y"}], "]"}]}]}], ")"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tdstrans", "=", 
      RowBox[{"Simplify", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"tds", "/.", 
          RowBox[{"{", 
           RowBox[{"f", "\[Rule]", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"u", "[", 
               RowBox[{
                RowBox[{"ArcTan", "[", 
                 RowBox[{"#1", "/", "c"}], "]"}], ",", 
                RowBox[{"ArcTan", "[", 
                 RowBox[{"#2", "/", "c"}], "]"}]}], "]"}], "&"}], ")"}]}], 
           "}"}]}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x", "\[Rule]", 
            RowBox[{"(", 
             RowBox[{"c", "*", 
              RowBox[{"Tan", "[", "\[Xi]", "]"}]}], ")"}]}], ",", 
           RowBox[{"y", "\[Rule]", 
            RowBox[{"(", 
             RowBox[{"c", "*", 
              RowBox[{"Tan", "[", "\[Psi]", "]"}]}], ")"}]}]}], "}"}]}], ",", 
        
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Pi", "/", "2"}], ")"}], ">", "\[Xi]", ">", 
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{"Pi", "/", "2"}], ")"}]}]}], ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Pi", "/", "2"}], ")"}], ">", "\[Psi]", ">", 
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{"Pi", "/", "2"}], ")"}]}]}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"evs", ",", "efs"}], "}"}], "=", 
      RowBox[{"NDEigensystem", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"tdstrans", ",", 
          RowBox[{"DirichletCondition", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"u", "[", 
              RowBox[{"\[Xi]", ",", "\[Psi]"}], "]"}], "\[Equal]", "0"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"Abs", "[", "\[Xi]", "]"}], "\[Equal]", 
              RowBox[{"(", "stranseps", ")"}]}], "||", 
             RowBox[{
              RowBox[{"Abs", "[", "\[Psi]", "]"}], "\[Equal]", 
              RowBox[{"(", "stranseps", ")"}]}]}]}], "]"}]}], "}"}], ",", 
        RowBox[{"u", "[", 
         RowBox[{"\[Xi]", ",", "\[Psi]"}], "]"}], ",", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[Xi]", ",", "\[Psi]"}], "}"}], "\[Element]", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "stranseps"}], ",", 
             RowBox[{"-", "stranseps"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"stranseps", ",", "stranseps"}], "}"}]}], "]"}]}], ",", 
        "nmax", ",", 
        RowBox[{"Method", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\"\<SpatialDiscretization\>\"", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{"\"\<FiniteElement\>\"", ",", 
              RowBox[{"{", 
               RowBox[{"\"\<MeshOptions\>\"", "\[Rule]", 
                RowBox[{"{", 
                 RowBox[{"\"\<MaxCellMeasure\>\"", "\[Rule]", "eps"}], 
                 "}"}]}], "}"}]}], "}"}]}], ",", 
           RowBox[{"\"\<Eigensystem\>\"", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{"\"\<Arnoldi\>\"", ",", 
              RowBox[{"\"\<MaxIterations\>\"", "\[Rule]", "maxi"}]}], "}"}]}],
            ",", 
           RowBox[{"\"\<VectorNormalization\>\"", "\[Rule]", "vn"}]}], 
          "}"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"evs", "-", "shift"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Function", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], ",", 
           RowBox[{"Evaluate", "[", 
            RowBox[{
             RowBox[{"Head", "[", 
              RowBox[{"efs", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}], "[", 
             RowBox[{
              RowBox[{"ArcTan", "[", 
               RowBox[{"x", "/", "c"}], "]"}], ",", 
              RowBox[{"ArcTan", "[", 
               RowBox[{"y", "/", "c"}], "]"}]}], "]"}], "]"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nmax"}], "}"}]}], "]"}]}], "}"}]}]}], 
   "]"}]}]], "Input",
 InitializationCell->True,
